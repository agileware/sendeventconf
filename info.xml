<?xml version="1.0"?>
<extension key="eu.tttp.sendeventconf" type="module">
  <file>sendeventconf</file>
  <name>send event conf</name>
  <description>This extension changes the default so it doesn't send a confirmation email when registering from the back office. Agileware fork for CiviX upgrades.</description>
  <license>agpl3</license>
  <maintainer>
    <author>Xavier dutoit</author>
    <email>civicrm@tttp.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/sendeventconf</url>
    <url desc="Documentation">https://github.com/agileware/sendeventconf</url>
    <url desc="Support">http://forum.civicrm.org</url>
    <url desc="Licensing">http://civicrm.org/licensing</url>
  </urls>
  <releaseDate>2026-02-17</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Sendeventconf</namespace>
    <format>25.10.2</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
</extension>
